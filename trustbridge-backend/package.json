{
  "name": "trustbridge-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node --max-old-space-size=4096 ./node_modules/@nestjs/cli/bin/nest.js build && npm run copy:artifacts",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/@nestjs/cli/bin/nest.js build --webpack && npm run copy:artifacts",
    "copy:artifacts": "mkdir -p dist/contracts/artifacts && cp -r contracts/artifacts/* dist/contracts/artifacts/ 2>/dev/null || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=2048 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nest start --watch",
    "minimal": "npx ts-node src/minimal-server.ts",
    "setup:api-keys": "node scripts/get-api-keys.js",
    "test:api": "node scripts/test-real-apis.js",
    "test:free": "node scripts/test-free-apis.js",
    "test:flow": "node scripts/test-complete-flow.js",
    "test:flow:detailed": "node scripts/test-complete-user-flow-detailed.js",
    "beta:setup": "npm run setup:api-keys && npm run test:api",
    "free:setup": "cp config.free-beta.env .env && npm run test:free",
    "free:test": "npm run free:setup && npm run test:flow",
    "free:test:detailed": "npm run free:setup && npm run test:flow:detailed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
